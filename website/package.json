{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:no": "docusaurus start --no-open",
    "build": "docusaurus build --out-dir build/team-forge",
    "build:fast": "PREVIEW_DEPLOY=true yarn run build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clean": "docusaurus clear",
    "test": "yarn build",
    "version:cut": "docusaurus docs:version",
    "sync:constants": "docusaurus sync-constants",
    "format:source": "yarn dlx prettier --write {{core,src}/**/*.js,*.js,{core,src}/**/*.jsx,*.jsx}",
    "format:markdown": "yarn dlx prettier --write ../docs/**/*.md && prettier --write {versioned_docs/**/*.md,blog/**/*.md}",
    "format:style": "yarn dlx prettier --write src/**/*.{scss,css}",
    "format:examples": "eslint-examples-js --fix && eslint-examples-tsx --fix",
    "prettier": "yarn format:source && yarn format:markdown && yarn format:style",
    "lint": "eslint ../docs/** blog/** core/** src/**/*.js ./*.js",
    "lint:examples": "eslint-examples-js && eslint-examples-tsx && tsc-examples",
    "lint:versioned": "yarn dlx eslint versioned_docs/**/*.{md,mdx}",
    "lint:markdown": "remark ../docs --quiet -r",
    "ci:lint": "yarn lint && yarn lint:examples && yarn lint:versioned && yarn language:lint:versioned && yarn lint:markdown",
    "pwa:generate": "npx pwa-asset-generator ./static/img/header_logo.svg ./static/img/pwa --padding '40px' --background 'rgb(32, 35, 42)' --icon-only --opaque true",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "^3.2.1",
    "@docusaurus/logger": "^3.2.1",
    "@docusaurus/plugin-pwa": "^3.2.1",
    "@docusaurus/preset-classic": "3.2.1",
    "@mdx-js/react": "^3.0.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.1.7",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.2.1",
    "@docusaurus/types": "3.2.1",
    "@types/react": "18.3.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
